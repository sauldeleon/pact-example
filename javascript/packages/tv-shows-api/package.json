{
  "name": "tv-shows-api",
  "version": "0.0.2",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "test:consumer": "jest --runInBand --setupFiles ./pact/setup.js --setupFilesAfterEnv ./pact/jest-wrapper.js --testMatch \"**/*.test.pact.js\"",
    "pact:verify": "node ./pact/provider_tests.js",
    "test:provider": "PACT_MODE=true start-server-and-test start http://localhost:3000 pact:verify",
    "test:pact": "run-s test:consumer test:provider",
    "test": "jest --setupFilesAfterEnv ./setupTests.js --testMatch \"**/*.test.js\"",
    "publish:pact": "node pact/publish.js",
    "ncu": "ncu"
  },
  "dependencies": {
    "axios": "^0.24.0",
    "cookie-parser": "~1.4.5",
    "debug": "~4.3.2",
    "express": "~4.17.1",
    "morgan": "~1.10.0",
    "node-fetch": "2.6.6",
    "npm-check-updates": "12.0.2"
  },
  "devDependencies": {
    "@pact-foundation/pact": "9.16.5",
    "mockttp": "^2.4.0",
    "msw": "^0.35.0",
    "npm-run-all": "^4.1.5",
    "prettier": "2.4.1",
    "start-server-and-test": "1.14.0"
  }
}
