{
  "name": "tv-shows-provider",
  "version": "0.0.2",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "pact:providerTests": "node ./pact/provider_tests.js",
    "pact:consumerTests": "jest --runInBand --setupFiles ./pact/setup.js --setupFilesAfterEnv ./pact/jest-wrapper.js --testMatch \"**/*.test.pact.js\"",
    "test:pact": "PACT_MODE=true start-server-and-test start http://localhost:3000 pact:providerTests",
    "publish:pact": "node pact/publish.js",
    "ncu": "ncu"
  },
  "dependencies": {
    "cookie-parser": "~1.4.5",
    "debug": "~4.3.2",
    "express": "~4.17.1",
    "morgan": "~1.10.0",
    "node-fetch": "2.6.6",
    "npm-check-updates": "12.0.2"
  },
  "devDependencies": {
    "@pact-foundation/pact": "9.16.5",
    "prettier": "2.4.1",
    "start-server-and-test": "1.14.0"
  }
}
